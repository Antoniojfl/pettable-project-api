{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 20",
    "_version": "20.1.0",
  
    "compilerOptions": {
      "lib": ["ES2022"],
      "module": "ES2022",
      "target": "ES2022",
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "Bundler"
    }
  }